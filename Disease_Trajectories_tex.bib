	keywords = {Big Data, Big data analytics, CNN-based multimodal disease risk prediction algorithm, convolutional neural network-based multimodal disease risk prediction algorithm, Data models, disease prediction, Diseases, health care, healthcare, healthcare communities, Hospitals, learning (artificial intelligence), machine learning, machine learning algorithms, Machine learning algorithms, medical big data analytics, neural nets, Prediction algorithms, real-life hospital data},
	pages = {8869--8879},
